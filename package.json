{
  "name": "demo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@comunica/actor-init-sparql": "^1.10.0",
    "@reach/router": "^1.3.3",
    "highcharts": "^8.0.1",
    "highcharts-react-official": "^3.0.0",
    "purgecss": "^2.1.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-scripts": "3.4.0",
    "tailwindcss": "^1.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.1",
    "@testing-library/user-event": "^10.0.0"
  },
  "scripts": {
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start": "yarn css:dev && react-scripts start",
    "build": "yarn css:prod && react-scripts build",
    "css:dev": "tailwind build ./src/theme/index.css -c ./tailwind.config.js -o ./src/theme/theme-generated.css",
    "css:prod": "yarn css:dev && purgecss --config ./purgecss.config.js --output ./src/theme"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
